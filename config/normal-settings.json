{
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": true,
        "json": true
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.editor.enableCodeActions": true,
    "github.copilot.chat.enableAutoSuggestions": false,
    "github.copilot.chat.autoAcceptSuggestions": false,
    "chat.editor.autoAcceptInlineChat": false,
    "chat.editor.autoReplies": {},
    "terminal.integrated.autoReplies": {},
    "chat.maxRequestsPerSession": 30,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.showToolbar": "onHover",
    "git.autofetch": true,
    "files.autoSave": "off",
    "[swift]": {
        "editor.defaultFormatter": "swift-lang.swift",
        "editor.formatOnSave": false
    }
}